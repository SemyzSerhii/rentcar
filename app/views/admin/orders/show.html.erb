<div class="media size-orders-show">
  <main class="show-maim-order">
    <div class="show-display-order">
      <div class="show-table-order">
        <table class="table table-bordered">
          <thead>
          </thead>
          <tbody>
          <tr><td>Id:</td><td><%= @order.id %></td></tr>
          <tr><td>Salon id:</td><td><%= @order.salon_id %></td></tr>
          <tr><td>Salon name:</td><td><%= @order.salon_name %></td></tr>
          <tr><td>Car id:</td><td><%= @order.car_id%></td></tr>
          <tr><td>Car brand:</td><td><%= @order.car_brand %></td></tr>
          <tr><td>Car model:</td><td><%= @order.car_model %></td></tr>
          <tr><td>Customer id:</td><td><%= @order.customer_id %></td></tr>
          <tr><td>Customer name:</td><td><%= @order.customer_name %></td></tr>
          <tr><td>Created At:</td><td><%= @order.created_at%></td></tr>
          <tr><td>Updated At:</td><td><%= @order.updated_at%></td></tr>
          </tbody>
        </table>
      </div>
      <div class="link-show-order">
        <%= link_to 'Delete', admin_order_path, method: :delete, class: 'button_show-order button-margin-order' %>
      </div>
    </div>
  </main>
</div>
