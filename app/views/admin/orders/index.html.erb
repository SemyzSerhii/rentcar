<div class="index-orders">
  <main class="table-tb">
    <div class="table-logo">
      <div class="logo">
        <h4>Orders Table</h4>
      </div>
      <table class="table table-bordered margin_table">
        <thead>
        <tr>
          <th scope="col">Id</th>
          <th scope="col">Cars id</th>
          <th scope="col">Customers id</th>
          <th scope="col">Created at</th>
          <th scope="col">Updated at</th>
        </tr>
        </thead>
        <tbody>
        <% @orders.each do |order| %>
          <tr onclick="window.location.href='<%= admin_order_path(order) %>'; return false"
              class="order-hover">
            <%= render order %>
          </tr>
        <% end %>
        </tbody>
      </table>
    </div>
  </main>
</div>